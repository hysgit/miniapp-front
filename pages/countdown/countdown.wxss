/* 样式文件 */
.container {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100vh;
}

text {
  font-size: 20px;
  color: #333;
}

.button-container {
  display: flex;
  justify-content: flex-start;
  padding: 10px;
}

button {
  background-color: #007aff;
  color: white;
  border: none;
  padding: 10px 20px;
  border-radius: 5px;
  font-size: 16px;
}
